<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<fx:root type="javafx.scene.layout.AnchorPane"
         prefHeight="550.0"
         prefWidth="800.0"
         xmlns:fx="http://javafx.com/fxml">
    <children>
        <VBox layoutX="20.0" layoutY="31.0" prefHeight="519.0" prefWidth="780.0" AnchorPane.bottomAnchor="10.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0">
                    <children>
                        <DatePicker fx:id="dpStart" promptText="Startdatum"/>
                        <DatePicker fx:id="dpSlut" promptText="Slutdatum"/>
                        <Button fx:id="btSubmit" mnemonicParsing="false" onAction="#loadAllAuktions" text="Sök"/>
                    </children>
                </HBox>

                <TableView fx:id="tableView" prefHeight="496.0" prefWidth="780.0">
                    <columns>
                        <TableColumn prefWidth="156.0" text="Namn">
                            <cellValueFactory>
                                <PropertyValueFactory property="produktNamn"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="156.0" text="Slutdatum">
                            <cellValueFactory>
                                <PropertyValueFactory property="slutDatum"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn minWidth="21.0" prefWidth="156.0" text="Högsta bud">
                            <cellValueFactory>
                                <PropertyValueFactory property="hogstaBud"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn minWidth="8.0" prefWidth="156.0" text="Beräknad provision">
                            <cellValueFactory>
                                <PropertyValueFactory property="beraknadProvision"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn minWidth="16.0" prefWidth="156.0" text="Provisionsandel">
                            <cellValueFactory>
                                <PropertyValueFactory property="provandel"/>
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                </TableView>
            </children>
            <padding>
                <Insets top="20.0"/>
            </padding>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
