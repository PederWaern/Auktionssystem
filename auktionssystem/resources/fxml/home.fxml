<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.surperfluousfew.auktionsystem.controllers.HomeController"
            prefHeight="600"
            prefWidth="800">
    <left>
        <GridPane>
            <children>
                <Button mnemonicParsing="false" onAction="#setSceneAddAuktion" text="Ny Auktion"
                        GridPane.rowIndex="0"/>
                <Button mnemonicParsing="false" onAction="#setSceneAddKund" text="Ny Kund"
                        GridPane.rowIndex="1"/>
                <Button mnemonicParsing="false" onAction="#setSceneAddLeverantor" text="Ny Leverantor"
                        GridPane.rowIndex="2"/>
                <Button mnemonicParsing="false" onAction="#setScenePGA"
                        text="Pågående Auktioner" GridPane.rowIndex="3"/>
                <Button mnemonicParsing="false" onAction="#setSceneForsaljning" text="Försäljningsstatistik"
                        GridPane.rowIndex="4"/>
            </children>
        </GridPane>
    </left>

    <center>
        <GridPane fx:id="gpHome" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
    </center>

</BorderPane>
