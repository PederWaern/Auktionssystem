<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>


<GridPane fx:id="root" alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/8.0.112-ea" fx:controller="com.surperfluousfew.auktionsystem.controllers.HomeController">
   <children>
      <Button fx:id="bAddAuktion" mnemonicParsing="false" text="Nya Auktion" GridPane.rowIndex="0" onAction="#setSceneAddAuktion"/>
      <Button fx:id="bAddKund" mnemonicParsing="false" text="Ny Kund" GridPane.rowIndex="1" onAction="#setSceneAddKund"/>
      <Button fx:id="bAddLev" mnemonicParsing="false" text="Ny Leverantor" GridPane.rowIndex="2" onAction="#setSceneAddLeverantor"/>
      <Button fx:id="bPagaendeAuktioner" mnemonicParsing="false" text="Pågående Auktioner" GridPane.rowIndex="3" onAction="#setScenePGA"/>
      <Button fx:id="bBudHistorik" mnemonicParsing="false" text="Bud Historik" GridPane.rowIndex="4" onAction="#setSceneBudHistorik"/>
      <Button fx:id="bKundLista" mnemonicParsing="false" text="Visa Kundlista" GridPane.rowIndex="5" onAction="#setSceneKundLista"/>

   </children>
</GridPane>
