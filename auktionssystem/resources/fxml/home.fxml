<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml" prefHeight="600.0" prefWidth="800.0">
   <children>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <items>
              <MenuBar>
                  <menus>
                      <Menu mnemonicParsing="false" text="Lägg till">
                          <items>
                              <MenuItem mnemonicParsing="false" onAction="#setSceneAddAuktion" text="Auktion" />
                              <MenuItem mnemonicParsing="false" onAction="#setSceneAddKund" text="Kund" />
                              <MenuItem mnemonicParsing="false" onAction="#setSceneAddLeverantor" text="Leverantör" />
                          </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Vyer">
                          <items>
                              <MenuItem mnemonicParsing="false" onAction="#setScenePGA" text="Pågående auktioner" />
                          </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Försäljningsstatistik">
                          <items>
                              <MenuItem mnemonicParsing="false" onAction="#setSceneKundLista" text="Kunder" />
                              <MenuItem mnemonicParsing="false" onAction="#setSceneAuktioner" text="Auktioner" />
                              <MenuItem mnemonicParsing="false" onAction="#setSceneProvision" text="Provision" />
                          </items>
                      </Menu>
                  </menus>
              </MenuBar>
              
              <AnchorPane fx:id="root">
                  <opaqueInsets>
                      <Insets />
                  </opaqueInsets>
              </AnchorPane>
          </items>
      </SplitPane>
   </children>
</fx:root>
