<fx:root type="javafx.scene.layout.GridPane" alignment="center" hgap="10" prefHeight="550" prefWidth="800" vgap="10" xmlns:fx="http://javafx.com/fxml">
    <children>
        <ChoiceBox fx:id="cbProdukt" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />

        <Button fx:id="addAuction" onAction="#add" text="Lägg till" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />

        <Label text="Utgångspris:" GridPane.columnIndex="0" GridPane.rowIndex="1" />

        <TextField fx:id="txfUpris" GridPane.columnIndex="1" GridPane.rowIndex="1" />

        <Label text="Acceptpris:" GridPane.rowIndex="2" />

        <TextField fx:id="txfApris" GridPane.columnIndex="1" GridPane.rowIndex="2" />

        <DatePicker fx:id="dpStart" promptText="Startdatum" GridPane.rowIndex="3" />

        <DatePicker fx:id="dpSlut" promptText="Slutdatum" GridPane.columnIndex="1" GridPane.rowIndex="3" />

        <Text fx:id="tInfo" text="" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Label layoutX="163.0" layoutY="260.0" text="Produkt:" />

    </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
